{
    "manifest_version": 2,
    "name": "AnimePahe Timestamper",
    "description": "Logs timestamps for anime episdoes for later viewing",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "window.html"
    },
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ]
}